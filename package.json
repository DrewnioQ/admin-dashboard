{
  "name": "ts-project-template",
  "version": "1.0.2",
  "description": "Vite bundler with TypeScript & SASS compile|autoprefix|minimize",
  "main": "index.html",
  "type": "module",
  "author": "DrewnioQ",
  "scripts": {
    "dev": "vite serve --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "postbuild": "postcss dist/assets/*.css -u autoprefixer -r --no-map"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "modern-normalize": "^1.1.0",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.7",
    "sass": "^1.61.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
